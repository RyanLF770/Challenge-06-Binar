{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "plugin:react/jsx-runtime",
        "plugin:react-hooks/recommended"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "react-refresh"
    ],
    "rules": {
      "react-refresh/only-export-components": "warn",
      "linebreak-style": 0,
      "indent": 0,
      "require-jsdoc": 0,
      "max-len":0
    }
}
